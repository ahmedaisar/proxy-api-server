{
  "version": 2,
  "routes": [
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/search?$1"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "runtime": "bun"
    }
  }
}